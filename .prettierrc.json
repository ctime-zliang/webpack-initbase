{
	"printWidth": 150,
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"proseWrap": "preserve",
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"endOfLine": "auto",
	"htmlWhitespaceSensitivity": "css",
	"jsxBracketSameLine": true,
	"jsxSingleQuote": false,
	"trailingComma": "es5"
}
