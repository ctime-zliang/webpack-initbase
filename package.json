{
	"name": "webpack-initbase",
	"version": "1.0.0",
	"description": "**Only a test project**......",
	"main": "index.js",
	"directories": {
		"doc": "doc"
	},
	"scripts": {
		"git:precommit": "lint-staged",
		"husky:install": "husky install",
		"prettier:write-all": "prettier --write .",
		"prettier:check-all": "prettier --check .",
		"tsc:check": "tsc --noEmit",
		"webpack:dev": "cross-env NODE_ENV=development webpack server --config ./webpack/config/webpack.dev.config.js",
		"webpack:prod": "cross-env NODE_ENV=production webpack --config ./webpack/config/webpack.prod.config.js",
		"jest:test": "cross-env NODE_ENV=test jest",
		"jest:coverage": "cross-env NODE_ENV=test jest --coverage",
		"jest:puppeteer": "cross-env NODE_ENV=test jest --puppeteer=true",
		"jest:puppeteer-custom": "cross-env NODE_ENV=test jest --puppeteer=true --user-custom=true"
	},
	"husky": {
		"hooks": {
			"pre-commit": "git:precommit"
		}
	},
	"lint-staged": {
		"*.{js, ts, tsx, jsx}": [
			"prettier:write-all",
			"git add ."
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ctime-zliang/webpack-initbase.git"
	},
